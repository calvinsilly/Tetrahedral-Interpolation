 addUserKnob {22 calculate l Calculate T "from sympy import *\
 
n = nuke.thisNode()
 
bi = n.knob('bi').getValue()
gi = n.knob('gi').getValue()
ri = n.knob('ri').getValue()
yi = n.knob('yi').getValue()
mi = n.knob('mi').getValue()
ci = n.knob('ci').getValue()
 
bo = n.knob('bo').getValue()
go = n.knob('go').getValue()
ro = n.knob('ro').getValue()
yo = n.knob('yo').getValue()
mo = n.knob('mo').getValue()
co = n.knob('co').getValue()
 
r1, r2, r3, g1, g2, g3, b1, b2, b3, c1, c2, c3, m1, m2, m3, y1, y2, y3 = symbols('r1 r2 r3 g1 g2 g3 b1 b2 b3 c1 c2 c3 m1 m2 m3 y1 y2 y3')
 
result = solve( [-ro [0] + ri [0] * r1 + ri [1] * (y1 - r1) + ri [2] * (1-y1),\
-ro [1] + ri [0] * r2 + ri [1] * (y2 - r2) + ri [2] * (1-y2),\
-ro [2] + ri [0] * r3 + ri [1] * (y3 - r3) + ri [2] * (1-y3),\
-go [0] + gi [0] * (y1-g1) + gi [1] * g1 + gi [2] * (1-y1),\
-go [1] + gi [0] * (y2-g2) + gi [1] * g2 + gi [2] * (1-y2),\
-go [2] + gi [0] * (y3-g3) + gi [1] * g3 + gi [2] * (1-y3),\
-bo [0] + bi [0] * (1-c1) + bi [1] * (c1-b1) + bi [2] * b1,\
-bo [1] + bi [0] * (1-c2) + bi [1] * (c2-b2) + bi [2] * b2,\
-bo [2] + bi [0] * (1-c3) + bi [1] * (c3-b3) + bi [2] * b3,\
-co [0] + ci [0] * (1-c1) + ci [1] * (c1-b1) + ci [2] * b1,\
-co [1] + ci [0] * (1-c2) + ci [1] * (c2-b2) + ci [2] * b2,\
-co [2] + ci [0] * (1-c3) + ci [1] * (c3-b3) + ci [2] * b3,\
-mo [0] + mi [0] * r1 + mi [1] * (1-m1) + mi [2] * (m1-r1),\
-mo [1] + mi [0] * r2 + mi [1] * (1-m2) + mi [2] * (m2-r2),\
-mo [2] + mi [0] * r3 + mi [1] * (1-m3) + mi [2] * (m3-r3),\
-yo [0] + yi [0] * r1 + yi [1] * (y1 - r1) + yi [2] * (1-y1),\
-yo [1] + yi [0] * r2 + yi [1] * (y2 - r2) + yi [2] * (1-y2),\
-yo [2] + yi [0] * r3 + yi [1] * (y3 - r3) + yi [2] * (1-y3)],  [r1, r2, r3, g1, g2, g3, b1, b2, b3, c1, c2, c3, m1, m2, m3, y1, y2, y3])
 
n.knob('red').setValue((result [r1],result [r2],result [r3]))
n.knob('grn').setValue((result [g1],result [g2],result [g3]))
n.knob('blu').setValue((result [b1],result [b2],result [b3]))
n.knob('cyn').setValue((result [c1],result [c2],result [c3]))
n.knob('mag').setValue((result [m1],result [m2],result [m3]))
n.knob('yel').setValue((result [y1],result [y2],result [y3]))
" +STARTLINE}
 addUserKnob {22 reset -STARTLINE T "n = nuke.thisNode()
 
n.knob('ri').setValue((1,0,0))
n.knob('gi').setValue((0,1,0))
n.knob('bi').setValue((0,0,1))
