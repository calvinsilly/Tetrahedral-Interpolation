n.knob('ro').setValue((1,0,0))
n.knob('go').setValue((0,1,0))
n.knob('bo').setValue((0,0,1))
n.knob('co').setValue((0,1,1))
n.knob('mo').setValue((1,0,1))
n.knob('yo').setValue((1,1,0))
 
n.knob('red').setValue((1,0,0))
n.knob('grn').setValue((0,1,0))
n.knob('blu').setValue((0,0,1))
n.knob('cyn').setValue((0,1,1))
n.knob('mag').setValue((1,0,1))
n.knob('yel').setValue((1,1,0))"}
